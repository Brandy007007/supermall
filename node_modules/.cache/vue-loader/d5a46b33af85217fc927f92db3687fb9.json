ass=\"info-detail\">\n      <p>{{ commentInfo.content }}</p>\n      <div class=\"info-other\">\n        <span class=\"date\">{{ commentInfo.created | showDate }}</span>\n        <span>{{ commentInfo.style }}</span>\n      </div>\n      <div class=\"info-imgs\">\n        <img\n          :src=\"item\"\n          v-for=\"(item, index) in commentInfo.images\"\n          :key=\"index\"\n          alt=\"\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { formatDate } from \"common/utils.js\";\nexport default {\n  name: \"DetailCommentInfo\",\n  props: {\n    commentInfo: {\n      type: Object,\n      default() {\n        return {};\n      },\n    },\n  },\n  filters: {\n    showDate: function (value) {\n      let date = new Date(value * 1000);\n      return formatDate(date, \"yyyy-MM-dd\");\n    },\n  },\n};\n</script>\n\n<style>\n.comment-info {\n  padding: 5px 12px;\n  color: #333333;\n  border-bottom: 5px solid #f2f5f8;\n}\n.info-header {\n  height: 50px;\n  line-height: 50px;\n  border-bottom: 1px solid rgba(0, 0, 0, 0.1);\n}\n.header-title {\n  float: left;\n  font-size: 15px;\n}\n.header-more {\n  float: right;\n  margin-right: 10px;\n  font-size: 13px;\n}\n.info-user {\n  pad: 10px 0 5px;\n}\n.info-user img {\n  width: 42px;\n  height: 42px;\n  border-radius: 50%;\n}\n.info-user span {\n  position: relative;\n  font-size: 15px;\n  top: -15px;\n  margin-left: 10px;\n}\n.info-detail {\n  padding: 0 5px 15px;\n}\n.info-detail p {\n  font-size: 14px;\n  counter-reset: #777;\n  line-height: 1.5;\n}\n.info-detail .info-other {\n  font-size: 12px;\n  color: #999;\n  margin-top: 10px;\n}\n.info-other date {\n  margin-right: 8px;\n}\n.info-imgs {\n  margin-top: 10px;\n}\n.info-imgs img {\n  width: 70px;\n  height: 70px;\n  margin-right: 5px;\n}\n</style>"]}]}